{
  "name": "project",
  "description": "Project starter for elizaOS",
  "version": "1.0.0-beta.51",
  "type": "module",
  "private": true,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@elizacn/plugin-mcp": "0.0.5",
    "@elizacn/plugin-vara": "0.0.19",
    "@elizaos/cli": "1.0.0-beta.51",
    "@elizaos/core": "1.0.0-beta.51",
    "@elizaos/plugin-bootstrap": "1.0.0-beta.51",
    "@elizaos/plugin-openai": "1.0.0-beta.51",
    "@elizaos/plugin-sql": "1.0.0-beta.51",
    "@gear-js/api": "^0.41.0",
    "@walletconnect/keyvaluestorage": "^1.1.1",
    "@walletconnect/sign-client": "^2.19.2",
    "qrcode-generator": "^1.4.4",
    "zod": "3.24.2"
  },
  "devDependencies": {
    "prettier": "3.5.3",
    "tsup": "8.4.0",
    "typescript": "^5.8.3"
  },
  "scripts": {
    "start": "npx @elizaos/cli start",
    "dev": "npx @elizaos/cli dev",
    "build": "tsup",
    "lint": "prettier --write ./src",
    "test": "npx @elizaos/cli test",
    "deploy": "npx @elizaos/cli deploy",
    "format": "prettier --write ./src",
    "format:check": "prettier --check ./src"
  },
  "publishConfig": {
    "access": "public"
  }
}
